{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug so_long (GDB)",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/so_long",
      "args": [],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "miDebuggerPath": "/usr/bin/gdb",
      "preLaunchTask": "build debug",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    },
    {
      "name": "Run so_long (no debugger)",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/so_long",
      "args": [],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "externalConsole": false,
      "MIMode": "gdb",
      "miDebuggerPath": "/usr/bin/gdb",
      "preLaunchTask": "build"
    }
  ]
}
